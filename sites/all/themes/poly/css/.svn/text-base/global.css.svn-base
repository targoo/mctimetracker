/*BREADCRUMB*/
#breadcrumb {
  margin: 0 0 10px;
}
#breadcrumb a {
    background: none repeat scroll 0 0 #DDDDDD;
    color: #444444;
    float: left;
    padding: 0.2em 1.2em;
    position: relative;
    text-decoration: none;
    text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
}
#breadcrumb a:hover {
  background: #efc9ab;
}
#breadcrumb .breadcrumb-inner{
  margin: 0;
  padding: 0;
  list-style: none;
  overflow: hidden;
  width: 100%;
}
.breadcrumb-inner li{
  float: left;
  margin: 5px 15px;
}
#breadcrumb a:before, #breadcrumb a:after {
    -moz-transform: skew(-10deg);
    background: none repeat scroll 0 0 #DDDDDD;
    bottom: 0;
    content: "";
    position: absolute;
    top: 0;
    width: 1em;
}
#breadcrumb a:hover:before, #breadcrumb a:hover:after {
  background: #efc9ab;
}
#breadcrumb a:after {
    border-radius: 0 5px 5px 0;
    right: -0.5em;
}
#breadcrumb a:before {
    border-radius: 5px 0 0 5px;
    left: -0.5em;
}



