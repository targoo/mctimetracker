.panel-block{
	padding: 30px;
	border-color: #DDDDDD #EEEEEE #DDDDDD;
    border-style: solid;
    border-width: 1px;
}

.size1of1 {
    display: block;
    float: none;
    width: 100%;
}
.size1of2 {
    margin-right: 5%;
    width: 45%;
}
.size1of3 {
    width: 33.3333%;
}
.size2of3 {
    width: 66.6667%;
}
.size1of4 {
    width: 25%;
}
.size3of4 {
    width: 75%;
}
.size1of5 {
    width: 20%;
}
.size2of5 {
    width: 40%;
}
.size3of5 {
    width: 60%;
}
.size4of5 {
    width: 80%;
}

.unit {
    float: left;
    overflow: hidden;
    min-height: 90px;
}

h3 {
    color: #3F3F38;
    font-size: 20px;
    line-height: 1;
    margin: 2em 0 0.5em;
    text-shadow: 1px 1px 0 #FFFFFF;
    border-bottom: 1px solid #DDDDDD;
    display: block;
    padding-bottom: 8px;
}

.resume .big {
    color: #3F3F38;
    font-size: 60px;
    padding:20px 0;
}
.resume .label {
    color: #3F3F38;
    font-size: 20px;
    float:left;
    padding:10px 10px;
    width:100%;
}

.gettingstarted {
    padding: 30px;
}
.gettingstarted a {
    background: url("../images/steps_grey.jpg") no-repeat scroll 0 0 transparent;
    display: block;
    font-size: 1px;
    height: 170px;
    text-indent: -9999px;
}
.gettingstarted a:hover {
    background: url("../images/steps.jpg") no-repeat scroll 0 0 transparent;
    display: block;
    font-size: 1px;
    height: 170px;
    text-indent: -9999px;
}
.gettingstarted a.gs1 {
    background-position: center 0;
}
.gettingstarted a.gs2 {
    background-position: center -200px;
}
.gettingstarted a.gs3 {
    background-position: center -400px;
}
